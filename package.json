{
  "name": "fractal-calm",
  "version": "2.0.0",
  "description": "Visual meditation app with infinite fractal visuals, guided breathing, and ambient audio",
  "main": "index.js",
  "scripts": {
    "serve": "npx http-server www -p 8000 -c-1",
    "icons": "node scripts/generate-icons.js",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:build": "npx cap sync && cd android && ./gradlew assembleRelease",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android"
  },
  "keywords": [
    "meditation",
    "mindfulness",
    "relaxation",
    "fractal",
    "breathing",
    "sleep",
    "calm",
    "wellness",
    "mental-health"
  ],
  "author": "nabz0r",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nabz0r/fractal-explorer.git"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@capacitor/haptics": "^7.0.0"
  },
  "devDependencies": {
    "sharp": "^0.33.0"
  }
}